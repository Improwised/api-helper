{
    "name": "lde/api-helper",
    "description": "Laravel package to help consume REST and XML APIs with little effort.",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "php": ">=7.0.0",
        "spatie/array-to-xml": "^2.8",
        "phpunit/phpunit": "^8.1",
        "adbario/php-dot-notation": "2.x-dev"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Lde\\ApiHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lde\\ApiHelper\\ApiHelperServiceProvider"
            ],
            "aliases": {
                "ApiHelper": "Lde\\ApiHelper\\ApiHelperFacade"
            }
        }
    }
}
